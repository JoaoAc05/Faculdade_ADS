Algoritmo "Temperatura"

Var
   celsius, fahrenheit, kelvin : real


Inicio
      escreval(" Quantos graus Celsius?")
      leia(celsius)
      fahrenheit <- (celsius * 1.8) + 32
      kelvin <- celsius + 273.15
      escreval(fahrenheit, " fahrenheit's")
      escreval(kelvin, " kelvin")

Fimalgoritmo